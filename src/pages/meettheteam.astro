<style>

 /*BREADCRUMBS*/    
        .BreadCrumbSection{
display: flex;
gap: 5px;
flex-direction: column;
align-self: flex-start;
padding-top: 30px;
margin-left: 20px;
    }

    .BreadCrumbSection a{
        font-family:var(--font-display);
        font-size: var(--font-size-small);
    }

    .tilbagesection{
        display: flex;
    }

    .breadcrumblinks {
        display: flex;
        list-style: none;
        gap: 10px;
    }

    /*GENERAL*/ 
    .moedteametlayout{
        display: flex;
        margin: 30px 50px;
        gap: 40px;
        flex-direction: column;
        justify-content: center;
    }
 
    /*SECTIONS*/ 
    .sub-header-underlined{
font-family: var(--font-display);
font-weight: var(--font-weight-semibold);
font-size: var(font-size-large);
align-self: flex-start;
border-bottom: 2px solid var(--color-black-ish);
width: 100%;
margin-bottom: 20px;
}

   .employeesection{
    display: flex;
    flex-wrap: wrap;
    gap: 20px;
   }

   @media only screen and (min-width: 400px){
.moedteametlayout{
    margin: 40px 80px;
}
   }

   @media only screen and (min-width: 900px){
.moedteametlayout{
    margin: 40px 140px;
}
   }
</style>